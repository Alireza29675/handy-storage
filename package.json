  "bugs": {
    "url": "https://github.com/Alireza29675/handy-storage/issues"
  },
  "homepage": "https://github.com/Alireza29675/handy-storage#readme",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "json-beautify": "^1.0.1"
  }
}
