{
  "name": "handy-storage",
  "version": "2.1.6",
  "description": "HandyStorage is a simple way to store your data in a JSON file",
  "main": "lib/index.js",
  "types": "./main.d.ts",
  "scripts": {
    "start": "npm run compile && node examples/index.js",
    "start:watch": "nodemon run examples/index.js",
